{
  "schemaVersion": 1,
  "id": "meteor-translation-plus",
  "version": "${version}",
  "name": "Meteor Translation+",
  "description": "An addon offers unicode font rendering fix and translation for Meteor, Meteor Rejects and Meteor+",
  "authors": [
    "Nippaku Zanmu",
    "E0x72-21"
  ],
  "contact": {
    "repo": "https://github.com/E0x72-21/meteor-translation-addon"
  },
  "icon": "assets/meteor-translation-addon/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.nippaku_zanmu.trans_addon.MeteorTranslation"
    ]
  },
  "mixins": [
    "addon-translation.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "231,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": [">=1.21"],
    "meteor-client": "*"
  }
}
